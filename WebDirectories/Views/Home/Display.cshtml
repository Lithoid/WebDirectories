@{
}
@model DisplayViewModel

<h1>@Model.CurrentFolder</h1>
@foreach (var item in Model.Folders)
{
    <p><a asp-action="Display" asp-route-currentId="@item.Id">@item.Name</a></p>
}
<a class="btn btn-success" asp-action="Import">Import</a>
<a class="btn btn-success" asp-action="ExportFromDb">Export</a>